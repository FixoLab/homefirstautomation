{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "headers": {
          "cache-control": "s-maxage=31536000, max-age=31536000"
        },
        "dest": "/api/handler.js"
      },
      {
        "src": "/(.*)",
        "headers": {
          "cache-control": "s-maxage=31536000, max-age=31536000"
        },
        "dest": "/index.html"
      }
    ]
  }
  