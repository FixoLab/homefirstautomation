{

"public": "build",
"headers": [
{
    "source": "/**",
    "headers": [
    {
        "key": "Cache-Control",
        "value": "no-cache, no-store, must-revalidate"
    }
    ]
},
{
    "source":
    "**/*.@(jpg|jpeg|gif|png|svg|mp4|webp|js|css|eot|otf|ttf|ttc|woff|woff2|font.css)",
    "headers": [
    {
        "key": "Cache-Control",
        "value": "max-age=1209600"
    }
    ]
}
],

"rewrites": [
{
    "source": "**",
    "destination": "/index.html"
}
]

  }